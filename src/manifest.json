{
    "name": "URLink",
    "version": "1.0",
    "description": "Chrome extension to store your links with a single click",
    "permissions": ["storage", "tabs", "webNavigation"],
    "browser_action": {
        "default_title": "URLink",
        "default_popup": "index.html",
        "default_icon": {
            "16": "get_started16.png",
            "32": "get_started32.png",
            "48": "get_started48.png",
            "128": "get_started128.png"
        }
    },
    "icons": {
        "16": "get_started16.png",
        "32": "get_started32.png",
        "48": "get_started48.png",
        "128": "get_started128.png"
    },
    "manifest_version": 2
}
